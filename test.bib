read = {Yes},
rating = {0},
date-added = {2010-09-26T19:03:15GMT},
date-modified = {2015-11-17T14:59:36GMT},
abstract = {We report the discovery with the 100 m Green Bank Telescope of 65 ms radio pulsations from the X-ray pulsar J0205+6449 at the center of supernova remnant 3C 58, making this possibly the youngest radio pulsar known. From our observations at frequencies of 820 and 1375 MHz, the free-electron column density to PSR J0205+6449 is found to be 140.7+/-0.3 cm-3 pc. The barycentric pulsar period P and period derivative P determined from a phase-coherent timing solution are consistent with the values previously measured from X-ray observations. The averaged radio profile of PSR J0205+6449 consists of one sharp pulse of width ~3 ms ~ 0.05P. The pulsar is an exceedingly weak radio source, with a pulse-averaged flux density in the 1400 MHz band of ~45 $\mu$Jy and a spectral index of ~-2.1. Its radio luminosity of ~0.5 mJy kpc2 at 1400 MHz is lower than that of ~99% of known pulsars and is the lowest among known young pulsars.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2002ApJ...571L..41C&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2002/Camilo/The%20Astrophysical%20Journal%202002%20Camilo.pdf},
file = {{The Astrophysical Journal 2002 Camilo.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2002/Camilo/The Astrophysical Journal 2002 Camilo.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/341178}}
}

@article{Murray:2002dh,
author = {Murray, Stephen S and Slane, Patrick O and Seward, Fredrick D and Ransom, Scott M and Gaensler, Bryan M},
title = {{Discovery of X-Ray Pulsations from the Compact Central Source in the Supernova Remnant 3C 58}},
journal = {The Astrophysical Journal},
year = {2002},
volume = {568},
pages = {226},
month = mar,
affiliation = {AA(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138; ssm@head-cfa.harvard.edu), AB(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138; ssm@head-cfa.harvard.edu), AC(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138; ssm@head-cfa.harvard.edu), AD(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138; ssm@head-cfa.harvard.edu), AE(Center for Space Research, Massachusetts Institute of...},
doi = {10.1086/338766},
rating = {0},
date-added = {2010-09-26T19:33:45GMT},
date-modified = {2015-11-17T14:59:36GMT},
abstract = {We report on high time and spatial resolution observations of the supernova remnant (SNR) 3C 58 using the High Resolution Camera (HRC) on the Chandra X-Ray Observatory. These data show a pointlike central source from which we detect 65.68 ms pulsations at 6.7 $\sigma$ significance. We interpret these pulsations as corresponding to a young rotation-powered pulsar, PSR J0205+6449, that is associated with and powers SNR 3C 58. Analysis of archival Rossi X-Ray Timing Explorer (RXTE) data from 3 years earlier confirms these pulsations and allows us to determine a spin-down rate of P=1.93×10-13ss-1. Assuming a magnetic dipole model for PSR J0205+6449, we infer a surface magnetic field of 3.6×1012 G. The historical age for the pulsar (based on its identification with SN 1181) is 820 yr. Taken with the observed period and spin-down rate, we derive an initial spin period of ~60 ms.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2002ApJ...568..226M&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2002/Murray/The%20Astrophysical%20Journal%202002%20Murray.pdf},
file = {{The Astrophysical Journal 2002 Murray.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2002/Murray/The Astrophysical Journal 2002 Murray.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/338766}}
}

@article{Tanaka:2010bs,
author = {Tanaka, Shuta J and Takahara, Fumio},
title = {{A Model of the Spectral Evolution of Pulsar Wind Nebulae}},
journal = {The Astrophysical Journal},
year = {2010},
volume = {715},
pages = {1248},
month = jun,
affiliation = {AA(Department of Earth and Space Science, Graduate School of Science, Osaka University, 1-1 Machikaneyama-cho, Toyonaka, Osaka 560-0043, Japan tanaka@vega.ess.sci.osaka-u.ac.jp), AB(Department of Earth and Space Science, Graduate School of Science, Osaka University, 1-1 Machikaneyama-cho, Toyonaka, Osaka 560-0043, Japan)},
doi = {10.1088/0004-637X/715/2/1248},
rating = {0},
date-added = {2010-09-27T12:09:24GMT},
date-modified = {2015-11-17T14:59:36GMT},
abstract = {We study the spectral evolution of pulsar wind nebulae (PWNe) taking into account the energy injected when they are young. We model the evolution of the magnetic field inside a uniformly expanding PWN. Considering time-dependent injection from the pulsar and coolings by radiative and adiabatic losses, we solve the evolution of the particle distribution function. The model is calibrated by fitting the calculated spectrum to the observations of the Crab Nebula at an age of a thousand years. The spectral evolution of the Crab Nebula in our model shows that the flux ratio of TeV $\gamma$-rays to X-rays increases with time, which implies that old PWNe are faint in X-rays, but not in TeV $\gamma$-rays. The increase of this ratio is because the magnetic field decreases with time and is not because the X-ray emitting particles are cooled more rapidly than the TeV $\gamma$-ray emitting particles. Our spectral evolution model matches the observed rate of the radio flux decrease of the Crab Nebula. This result implies that our magnetic field evolution model is close to the reality. Finally, from the viewpoint of the spectral evolution, only a small fraction of the injected energy from the Crab Pulsar needs to go to the magnetic field, which is consistent with previous studies.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2010ApJ...715.1248T&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2010/Tanaka/The%20Astrophysical%20Journal%202010%20Tanaka.pdf},
file = {{The Astrophysical Journal 2010 Tanaka.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2010/Tanaka/The Astrophysical Journal 2010 Tanaka.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/715/2/1248}}
}

