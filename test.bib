year = {2014},
volume = {791},
number = {2},
pages = {87},
month = aug,
affiliation = {School of Astronomy and Space Science, Nanjing University, Nanjing 210093, China ; Department of Physics and Astronomy, University of Manitoba, Winnipeg R3T 2N2, Canada},
doi = {10.1088/0004-637X/791/2/87},
read = {Yes},
