read = {Yes},
rating = {0},
date-added = {2010-09-23T12:36:24GMT},
date-modified = {2015-11-17T14:59:35GMT},
abstract = {3C 58 is a young Crab-like supernova remnant. Historical evidence strongly suggests an association of the remnant with supernova SN 1181, which would make 3C 58 younger than the Crab Nebula. Recent Chandra observations have identified the young 65 ms pulsar J0205+6449 at its center, embedded in a compact nebula that, as we show here, appears to be confined by the pulsar wind termination shock. We present new Chandra observations of this compact nebula and the embedded pulsar that set strong upper limits on the thermal emission originating from the neutron star surface. These limits fall far below predictions of standard neutron star cooling, requiring the presence of exotic cooling processes in the neutron star core.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2002ApJ...571L..45S&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2002/Slane/The%20Astrophysical%20Journal%202002%20Slane.pdf},
