journal = {Astrophysical Journal v.463},
year = {1996},
volume = {463},
pages = {609},
month = jun,
affiliation = {Center for Astrophysics, MS 72, 60 Garden Street, Cambridge, MA 02138; NASA/Goddard Space Flight Center, Code 631, Greenbelt, MD 20771},
doi = {10.1086/177275},
read = {Yes},
rating = {0},
date-added = {2015-10-22T13:48:42GMT},
date-modified = {2015-10-22T13:55:30GMT},
abstract = {We present a study of the diffuse, high-energy gamma-ray emission detected in the outer Galaxy toward the Cepheus flare by the EGRET instrument on the Compton Gamma Ray Observatory. The region studied (l = 100$\,^{\circ}$ to 130$\,^{\circ}$, b = -5$\,^{\circ}$ to +32$\,^{\circ}$) contains the prominent local clouds of the Cepheus and Polaris flares and the molecular cloud complex associated with Cas A and NGC 7538, the largest in the Perseus arm. Gas along the line of sight is partitioned into three distance ranges. The gamma-ray emissivity and N(H2)/WCO ratio in the local range are found to be (1.84$\pm$0.10) x 10-26 s-1sr-1 and (0.92$\pm$0.14) x 1020 cm-2 (K km s-1)-1, respectively, both derived for the energy range E > 100 MeV. In the Perseus arm, these quantities are (1.11$\pm$0.09) x 10-26 s-1 sr-1 and (2.5$\pm$0.9) x 1020 (K km s-1)-1. The decrease of emissivity from the solar circle to the Perseus arm is significant, although the variation of N(H2)/WCO is not. No significant variations of these quantities within each distance range are found. The gamma-ray emissivity in the Perseus arm is much less than predicted by models that assume a simple coupling between cosmic-ray density and surface density of interstellar gas. The derived emissivity in the Perseus arm constrains the amount of molecular gas not detected in CO to be at most one-tenth the amount of atomic hydrogen. A bright, soft spectrum gamma-ray source in this field reported from analysis of COS B data is not seen by EGRET. The spectrum of the extragalactic isotropic emission toward Cepheus is consistent with findings from other regions that have been studied with EGRET data.},
url = {http://adsabs.harvard.edu/abs/1996ApJ...463..609D},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/1996/Digel/1996%20Digel.pdf},
file = {{1996 Digel.pdf:/Users/elunejava/Dropbox/Papers2/Articles/1996/Digel/1996 Digel.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/177275}}
}

@article{1996ApJ...458..561D,
author = {Digel, Seth W and Lyder, David A and Philbrick, Amy J and Puche, Daniel and Thaddeus, Patrick},
title = {{A Large-Scale CO Survey toward W3, W4, and W5}},
journal = {Astrophysical Journal v.458},
year = {1996},
volume = {458},
pages = {561},
month = feb,
affiliation = {Harvard-Smithsonian Center for Astrophysics, MS 72, 60 Garden Street, Cambridge, MA 02138; NASA Goddard Space Flight Center, Code 631, Greenbelt, MD 20771},
doi = {10.1086/176839},
read = {Yes},
rating = {0},
date-added = {2015-10-22T13:56:51GMT},
date-modified = {2015-10-22T13:59:00GMT},
abstract = {A CO J = 1-0 survey of the outer Galaxy toward W3, W4, and W5 is described. The bright molecular emission in the W3 region in the Perseus arm is prominent, and little emission is seen at greater Galactocentric distances. The extensive local emission in the survey contains the large molecular cloud complex associated with Cam OB1. Between the local and Perseus arm emission, a nearly empty gap in velocity is seen across the entire longitude range. On the assumptions that the kinematically derived width of the gap is the true width and that the X ratio is constant, the arm-interarm contrast of CO is ≥ 25:1 in surface density. A few clouds are detected beyond the Perseus arm, and only one cloud, already known, is detected at extreme Galactocentric distances, beyond the Perseus and outer arms. The cumulative mass distribution of the clouds in the Perseus arm has power-law index 065$\pm$0.15 and the clouds there are systematically less luminous in CO by a factor 3.5$\pm$2 than clouds near the solar circle. The distribution of molecular gas with Galactocentric distance in the outer Galaxy is compared with that of luminous Infrared Astronomy Satellite point sources that trace massive star-forming regions. The agreement is generally good, with evidence for enhanced star formation relative to the surface density of molecular gas in the W3 region and perhaps also in the outer arm.},
url = {http://adsabs.harvard.edu/abs/1996ApJ...458..561D},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/1996/Digel/1996%20Digel-1.pdf},
file = {{1996 Digel-1.pdf:/Users/elunejava/Dropbox/Papers2/Articles/1996/Digel/1996 Digel-1.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/176839}}
}

@article{2011ApJ...738..181M,
author = {Malyshev, Dmitry and Hogg, David W},
title = {{Statistics of gamma-ray point sources below the Fermi detection limit}},
journal = {arXiv.org},
year = {2011},
eprint = {1104.0010v3},
eprinttype = {arxiv},
eprintclass = {astro-ph.CO},
number = {2},
