@article{2010ApJ...710..309T,
author = {Temim, Tea and Slane, Patrick and Reynolds, Stephen P and Raymond, John C and Borkowski, Kazimierz J},
title = {{Deep Chandra Observations of the Crab-like Pulsar Wind Nebula G54.1+0.3 and Spitzer Spectroscopy of the Associated Infrared Shell}},
journal = {The Astrophysical Journal},
year = {2010},
volume = {710},
number = {1},
pages = {309--324},
month = feb,
affiliation = {Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138, USA; Department of Astronomy, School of Physics and Astronomy, University of Minnesota, MN, USA},
doi = {10.1088/0004-637X/710/1/309},
read = {Yes},
