title = {{Discovery of Radio Pulsations from the X-Ray Pulsar J0205+6449 in Supernova Remnant 3C 58 with the Green Bank Telescope}},
journal = {The Astrophysical Journal},
year = {2002},
volume = {571},
pages = {L41},
month = may,
affiliation = {AA(Columbia Astrophysics Laboratory, Columbia University, 550 West 120th Street, New York, NY 10027.), AB(National Radio Astronomy Observatory, P.O. Box 2, Green Bank, WV 24944.), AC(University of Manchester, Jodrell Bank Observatory, Macclesfield, Cheshire SK11 9DL, UK.), AD(Department of Astronomy, University of California, 601 Campbell Hall, Berkeley, CA 94720-3411.), AE(Department of Physics, McGill University, Montreal, QC H3A 2T8, Canada.; Center for Space Research, Massachusetts Institute of Technolo...},
