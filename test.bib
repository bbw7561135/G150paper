@inproceedings{2008AIPC.1085..265G,
author = {Gabici, Stefano and Casanova, Sabrina and Aharonian, Felix A},
title = {{Non-thermal radiation from molecular clouds illuminated by cosmic rays from nearby supernova remnants.}},
