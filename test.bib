year = {2003},
volume = {144},
number = {1},
pages = {47--70},
month = jan,
affiliation = {National Research Council, Herzberg Institute of Astrophysics, Dominion Radio Astrophysical Observatory, P.O. Box 248, Penticton, BC, V2A 6K3 Canada; Department of Physics and Astronomy, University of Calgary, 2500 University Drive NW, Calgary, AB, T2N 1N4 Canada.},
