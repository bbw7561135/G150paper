@article{2014arXiv1402.5088K,
author = {Kothes, Roland and Sun, Xiaohui and Reich, Wolfgang and Foster, Tyler},
title = {{G141.2+5.0, a new Pulsar Wind Nebula discovered in the Cygnus Arm of the Milky Way}},
year = {2014},
eprint = {1402.5088},
