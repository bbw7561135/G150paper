pages = {492--507},
month = nov,
affiliation = {Institute for Astrophysical Research at Boston University, 725 Commonwealth Avenue, Boston, MA 02215, USA; Space Telescope Science Institute, 3700 San Martin Drive, Baltimore, MD 21218, USA jduval@bu.edu},
