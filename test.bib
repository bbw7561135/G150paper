affiliation = {Dublin Institute for Advanced Studies, 31 Fitzwilliam Place, Dublin 2, Ireland},
doi = {10.1063/1.3076656},
read = {Yes},
rating = {0},
date-added = {2015-01-20T23:35:23GMT},
date-modified = {2015-10-04T21:22:41GMT},
abstract = {Molecular clouds are expected to emit non-thermal radiation due to cosmic ray interactions in the dense magnetized gas. Such emission is amplified if a cloud is located close to an accelerator of cosmic rays and if cosmic rays can leave the accelerator and diffusively reach the cloud. We consider the situation in which a molecular cloud is located in the proximity of a supernova remnant which is accelerating cosmic rays and gradually releasing them into the interstellar medium. We calculate the multiwavelength spectrum from radio to $\gamma$-rays which emerges from the cloud as the result of cosmic ray interactions. The total energy output is dominated by the $\gamma$-ray emission, which can exceed the emission from other bands by an order of magnitude or more. This suggests that some of the unidentified TeV sources detected so far, with no obvious or very weak counterpart in other wavelengths, might be associated with clouds illuminated by cosmic rays coming from a nearby source.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2008AIPC.1085..265G&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2008/Gabici/2008%20Gabici.pdf},
file = {{2008 Gabici.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2008/Gabici/2008 Gabici.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1063/1.3076656}}
}

