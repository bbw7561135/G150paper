year = {2005},
volume = {632},
pages = {920},
month = oct,
affiliation = {AA(Physics Department, North Carolina State University, Box 8202, Raleigh, NC 27695 don_ellison@ncsu.edu), AB(Department of Physics and Astronomy, Rutgers University, 136 Frelinghuysen Road, Piscataway, NJ 08854-8019 chenai@physics.rutgers.edu)},
doi = {10.1086/444449},
read = {Yes},
rating = {0},
date-added = {2012-04-15T23:34:46GMT},
date-modified = {2015-11-17T14:59:39GMT},
abstract = {The strong shocks in young supernova remnants (SNRs) should accelerate cosmic rays (CRs), and there is no doubt that relativistic electrons are produced in SNRs. However, direct and convincing evidence that SNRs produce CR nuclei has not yet been obtained and may, in fact, be long in coming if current $\gamma$-ray observatories do not see an unambiguous pion-decay feature. Nevertheless, the lack of an observed pion-decay feature does not necessarily mean that CR ions are not abundantly produced since ions do not radiate efficiently. If CR ions are produced efficiently by diffusive shock acceleration (DSA), their presence will modify the hydrodynamics of the SNR and produce morphological effects that can be clearly seen in radiation produced by electrons. We describe in some detail our CR-hydro model, which couples DSA with the remnant hydrodynamics, and the synchrotron emission expected for two distinct parameter sets representing Type Ia and Type II supernovae. Several morphological features emerge in radial profiles, including the forward shock precursor, which are observable with current X-ray observatories and should definitively show whether young SNRs produce CR ions efficiently or not. For the specific case of SN 1006 we conclude, as have others, that the extremely short X-ray scale heights observed near the outer shock argue convincingly for the efficient production of CR ions.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2005ApJ...632..920E&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2005/Ellison/The%20Astrophysical%20Journal%202005%20Ellison.pdf},
file = {{The Astrophysical Journal 2005 Ellison.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2005/Ellison/The Astrophysical Journal 2005 Ellison.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/444449}}
}

