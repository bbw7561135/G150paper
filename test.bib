journal = {The Astrophysical Journal},
year = {2009},
volume = {706},
pages = {1269},
month = dec,
affiliation = {AA(Harvard-Smithonian Center For Astrophysics, 60 Garden Street, Cambridge, MA 02138, USA ; Now at the Institut de Physique Nucleaire, IN2P3/CNRS, 15 rue Georges Clemenceau, 91400 Orsay, France.), AB(Harvard-Smithonian Center For Astrophysics, 60 Garden Street, Cambridge, MA 02138, USA ), AC(Institute of Astronomy, School of Physics, The University of Sydney, NSW 2006, Australia ), AD(Harvard-Smithonian Center For Astrophysics, 60 Garden Street, Cambridge, MA 02138, USA )},
doi = {10.1088/0004-637X/706/2/1269},
read = {Yes},
rating = {0},
date-added = {2010-05-25T19:38:21GMT},
date-modified = {2015-11-17T14:59:34GMT},
abstract = {We present a Chandra X-ray observation of the very high energy $\gamma$-ray source HESS J1640 - 465. We identify a point source surrounded by a diffuse emission that fills the extended object previously detected by XMM-Newton at the centroid of the HESS source, within the shell of the radio supernova remnant (SNR) G338.3 - 0.0. The morphology of the diffuse emission strongly resembles that of a pulsar wind nebula (PWN) and extends asymmetrically to the southwest of a point source presented as a potential pulsar. The spectrum of the putative pulsar and compact nebula are well characterized by an absorbed power-law model which, for a reasonable N H value of 14 × 1022 cm-2, exhibit an index of 1.1 and 2.5 respectively, typical of Vela-like PWNe. We demonstrate that, given the H I absorption features observed along the line of sight, the SNR and the H II surrounding region are probably connected and lie between 8 kpc and 13 kpc. The resulting age of the system is between 10 and 30 kyr. For a 10 kpc distance (also consistent with the X-ray absorption) the 2-10 keV X-ray luminosities of the putative pulsar and nebula are L PSR ~ 1.3 × 1033 d 2 10 kpc erg s-1 and L PWN ~ 3.9 × 1033 d 2 10 erg s-1 (d 10 = d/10 kpc). Both the flux ratio of L PWN/L PSR ~ 3.4 and the total luminosity of this system predict a pulsar spin-down power around \dot{E} \sim 4 \times 10^{36}\;erg\;s^{-1}. We finally consider several reasons for the asymmetries observed in the PWN morphology and discuss the potential association with the HESS source in terms of a time-dependent one-zone leptonic model.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2009ApJ...706.1269L&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2009/Lemiere/The%20Astrophysical%20Journal%202009%20Lemiere.pdf},
file = {{The Astrophysical Journal 2009 Lemiere.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2009/Lemiere/The Astrophysical Journal 2009 Lemiere.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/706/2/1269}}
}

@article{Ellison:2010iw,
author = {Ellison, Donald C and Patnaude, Daniel J and Slane, Patrick and Raymond, John},
title = {{Efficient Cosmic Ray Acceleration, Hydrodynamics, and Self-Consistent Thermal X-Ray Emission Applied to Supernova Remnant RX J1713.7-3946}},
journal = {The Astrophysical Journal},
year = {2010},
volume = {712},
pages = {287},
month = mar,
affiliation = {AA(Physics Department, North Carolina State University, Box 8202, Raleigh, NC 27695, USA don_ellison@ncsu.edu), AB(Smithsonian Astrophysical Observatory, MS-3, 60 Garden Street, Cambridge, MA 02138, USA), AC(Smithsonian Astrophysical Observatory, MS-3, 60 Garden Street, Cambridge, MA 02138, USA), AD(Smithsonian Astrophysical Observatory, MS-3, 60 Garden Street, Cambridge, MA 02138, USA)},
doi = {10.1088/0004-637X/712/1/287},
read = {Yes},
rating = {0},
date-added = {2010-05-25T19:36:17GMT},
date-modified = {2015-11-17T14:59:34GMT},
abstract = {We model the broadband emission from supernova remnant (SNR) RX J1713.7-3946 including, for the first time, a consistent calculation of thermal X-ray emission together with non-thermal emission in a nonlinear diffusive shock acceleration model. Our model tracks the evolution of the SNR including the plasma ionization state between the forward shock and the contact discontinuity. We use a plasma emissivity code to predict the thermal X-ray emission spectrum assuming the initially cold electrons are heated either by Coulomb collisions with the shock-heated protons (the slowest possible heating), or come into instant equilibration with the protons. For either electron heating model, electrons reach gsim107 K rapidly and the X-ray line emission near 1 keV is more than 10 times as luminous as the underlying thermal bremsstrahlung continuum. Since recent Suzaku observations show no detectable line emission, this places strong constraints on the unshocked ambient medium density and on the relativistic electron-to-proton ratio. For the uniform circumstellar medium (CSM) models that we consider, the low densities and high relativistic electron-to-proton ratios required to match the Suzaku X-ray observations definitively rule out pion decay as the emission process producing GeV-TeV photons. We show that leptonic models, where inverse-Compton scattering against the cosmic background radiation dominates the GeV-TeV emission, produce better fits to the broadband thermal and non-thermal observations in a uniform CSM.},
