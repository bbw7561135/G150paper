@article{Breitling:2009tx,
author = {Breitling, F},
title = {{Detection of VHE Gamma Radiation from the Pulsar Wind Nebula MSH 15-52 with H.E.S.S}},
journal = {eprint arXiv},
year = {2009},
volume = {0903},
pages = {2056},
month = mar,
annote = {178 pages, 128 figures; restored original page size using the latex geometry package as recommended by arXiv},
read = {Yes},
rating = {0},
date-added = {2009-10-13T12:54:22GMT},
date-modified = {2015-11-17T14:59:33GMT},
abstract = {This work reports on the discovery of HESS J1514-591, a VHE gamma-ray source found at the pulsar wind nebula (PWN) MSH 15-52 and its associated pulsar PSR B1509-58. The discovery was made with the High Energy Stereoscopic System (H.E.S.S.), which currently provides the most sensitive measurement in the energy range of about 0.2-100 TeV. This analysis is the first to include all H.E.S.S. data from observations dedicated to MSH 15-52. The corresponding flux above 1 TeV is (4.4+/-0.2stat+/-1.0syst) x 10^{-12}cm^{-2}s^{-1}. The energy spectrum obeys a power-law with a differential flux at 1 TeV of (5.8+/-0.2stat+/-1.3syst) x 10^{-12}cm^{-2}s^{-1}TeV^{-1} and a photon index of 2.32+/-0.04stat+/-0.10syst. The gamma-ray emission extends along the pulsar jet, previously resolved in X-rays. This becomes more apparent after image deconvolution. The emission region along the jet axis decreases with increasing energy. An upper limit for the pulsed gamma-ray flux from PSR B1509-58 was calculated. Additional discussions include: the system of MSH 15-52 and PSR B1509-58, theory and methods of VHE gamma-ray astronomy and H.E.S.S., the first (Richardson-Lucy) deconvolution of VHE gamma-ray maps, search for pulsed emission from pulsars. The results are discussed within the framework of PWNs and are explained by inverse Compton scattering of leptons. A hadronic component is not excluded, but its gamma-ray emission would not be significant. Moreover, it is concluded that advection is the dominant transport mechanism over diffusion in the magnetized flow of the pulsar wind from PSR B1509-58. A correlation analysis with the Chandra X-ray data suggests that gamma radiation is emitted from the region of PSR B1509-58, but not from the neighboring optical nebula RCW 89.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2009arXiv0903.2056B&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2009/Breitling/eprint%20arXiv%202009%20Breitling.pdf},
file = {{eprint arXiv 2009 Breitling.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2009/Breitling/eprint arXiv 2009 Breitling.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/B0711375-2E16-41A7-885C-91BD9A1B33A1}}
}

@article{2001A&A...380..695M,
author = {Mineo, T and Cusumano, G and Maccarone, M C and Massaglia, S and Massaro, E and Trussoni, E},
title = {{The hard X-ray emission from the complex SNR MSH 15-52 observed by BeppoSAX}},
journal = {Astronomy and Astrophysics},
year = {2001},
volume = {380},
pages = {695--703},
month = dec,
affiliation = {Istituto di Fisica Cosmica con Applicazioni all'Informatica, CNR, via Ugo La Malfa 153, 90146 Palermo, Italy},
doi = {10.1051/0004-6361:20011576},
rating = {0},
date-added = {2009-10-08T14:42:20GMT},
date-modified = {2015-10-04T21:22:40GMT},
abstract = {We present the results of a BeppoSAX observation of the Supernova Remnant MSH 15-52, associated with the pulsar PSR B1509-58, and discuss its main morphological and spectroscopic properties in the 1.6-200 keV energy range (MECS and PDS instruments). The two main structures of the remnant, the Southern Nebula, the plerion centered on the pulsar, and the Northern Nebula, are clearly visible in the MECS, with the former showing a much a harder spectrum. Furthermore, a diffuse extended emission surrounds the whole remnant up to ~ 17' from the center. Non-thermal flux is detected in the PDS up to 200 keV as well, and it appears that also in this energy range the emission is not concentrated in the central region around the pulsar. These data imply that the plerion extends up to a few tens of parsecs from the pulsar.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2001A%26A...380..695M&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2001/Mineo/Astronomy%20and%20Astrophysics%202001%20Mineo.pdf},
file = {{Astronomy and Astrophysics 2001 Mineo.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2001/Mineo/Astronomy and Astrophysics 2001 Mineo.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1051/0004-6361:20011576}}
}

@article{Brazier:1997un,
author = {Brazier, K T S and Becker, W},
title = {{High-resolution X-ray imaging of the supernova remnant MSH 15-52}},
journal = {Monthly Notices of the Royal Astronomical Society},
year = {1997},
volume = {284},
pages = {335},
annote = {(c) 1997 The Royal Astronomical Society},
affiliation = {AA(Department of Physics, University of Durham, South Road, Durham DH1 3LE ), AB(Max-Planck-Institut fur extraterrestrische Physik, 85740 Garching bei Munchen, Germany)},
rating = {0},
date-added = {2009-10-08T14:41:53GMT},
date-modified = {2015-11-17T14:59:33GMT},
abstract = {We present high-resolution ROSAT observations of PSR 1509-58 and the supernova remnant MSH 15-52. The pulsar itself is resolved from the surrounding synchrotron nebula as a 75 per cent pulsed point source. The synchrotron emission forms a broad, elongated cross centred on the pulsar, and an extrapolation of the north-western arm of the cross to the supernova remnant rim coincides with the RCW 89 H$\alpha$ nebula. More than half of the X-ray flux in RCW 89 is resolved into a partial ring of tight knots that closely follow the radio morphology of the region. We suggest that all of the X-ray emission in the field of PSR 1509-58 can be explained if the system is similar to the `torus' and `jets' of the Crab pulsar synchrotron nebula, but viewed at more than ~70 deg to the pulsar spin axis. For current theories, this inclination favours pulsar emission from the outer, rather than the inner, magnetosphere. The luminosity of RCW 89 is driven in our model by the pulsar wind, which suggests that the NW-SE axis of the synchrotron nebula marks a collimated outflow along the pulsar spin axis, making PSR 1509-58 the third example of an isolated pulsar with a polar outflow.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1997MNRAS.284..335B&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/1997/Brazier/Monthly%20Notices%20of%20the%20Royal%20Astronomical%20Society%201997%20Brazier.pdf},
file = {{Monthly Notices of the Royal Astronomical Society 1997 Brazier.pdf:/Users/elunejava/Dropbox/Papers2/Articles/1997/Brazier/Monthly Notices of the Royal Astronomical Society 1997 Brazier.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/74983FF0-F779-45B0-8C8C-A2DD0F8EE469}}
}

