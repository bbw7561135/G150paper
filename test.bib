journal = {The Astrophysical Journal},
year = {2011},
volume = {734},
pages = {86},
month = jun,
affiliation = {AA(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138, USA ; Departamento de F{\'\i}sica, Universidad Sim{\'o}n Bol{\'\i}var, Valle de Sartenejas, Apdo. 89000, Caracas 1080A, Venezuela), AB(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138, USA), AC(Sydney Institute for Astronomy, School of Physics A29, The University of Sydney, Sydney, NSW 2006, Australia), AD(Department of Physics and Astronomy, Rutgers University, 136 Frelinghuysen Road, Piscataway, NJ 0...},
