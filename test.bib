month = jan,
affiliation = {Dipartimento di Fisica, "M. Merlin" dell'Universit{\`a} e del Politecnico di Bari, I-70126 Bari, Italy ; Istituto Nazionale di Fisica Nucleare, Sezione di Bari, 70126 Bari, Italy ; francesco.giordano@ba.infn.it},
