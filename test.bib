file = {{arXiv 2012 Virkar.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2012/Virkar/arXiv 2012 Virkar.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/496C22B2-02D5-44AF-AFAC-599A121FD614}}
}

@article{Dubner:2015ty,
author = {Dubner, Gloria and Giacani, Elsa},
title = {{Radio emission from Supernova Remnants}},
journal = {arXiv.org},
year = {2015},
eprint = {1508.07294v1},
eprinttype = {arxiv},
eprintclass = {astro-ph.HE},
month = aug,
read = {Yes},
