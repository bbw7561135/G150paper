month = sep,
affiliation = {AA(Department of Physics, Yunnan University, Kunming, Yunnan 650011, China; National Astronomical Observatoires/Yunnan Observatory, Chinese Academy of Sciences, Kumming, Yunnan 650011, China. lizhang@ynu.edu.cn), AB(Department of Physics, Yunnan University, Kunming, Yunnan 650011, China lizhang@ynu.edu.cn)},
