uri = {\url{papers2://publication/uuid/831A371A-05DD-4896-A7BA-0B68A6CFAB00}}
}

@article{Slane:2008ka,
author = {Slane, P and Helfand, D J and Reynolds, S P and Gaensler, Bryan M and Lemiere, A and Wang, Z},
title = {{The Infrared Detection of the Pulsar Wind Nebula in the Galactic Supernova Remnant 3C 58}},
journal = {The Astrophysical Journal},
year = {2008},
volume = {676},
pages = {L33},
month = mar,
affiliation = {AA(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138.), AB(Columbia Astrophysics Laboratory, Columbia University, 550 West 120th Street, New York, NY 10027.), AC(Department of Physics, North Carolina State University, Box 8202, Raleigh, NC 27695-8202.), AD(School of Physics A29, University of Sydney, NSW 2006, Australia.), AE(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138.), AF(Department of Physics, McGill University, 3600 University Stre...},
doi = {10.1086/587031},
read = {Yes},
rating = {0},
date-added = {2010-09-23T12:35:50GMT},
date-modified = {2015-11-17T14:59:35GMT},
abstract = {We present infrared observations of 3C 58 with the Spitzer Space Telescope and the Canada-France-Hawaii Telescope. Using the IRAC camera, we have imaged the entire source, which results in clear detections of the nebula at 3.6 and 4.5 $\mu$m. The derived flux values are consistent with extrapolation of the X-ray spectrum to the infrared band, demonstrating that any cooling break in the synchrotron spectrum must occur near the soft X-ray band. We also detect the torus surrounding PSR J0205+6449, the 65 ms pulsar that powers 3C 58. The torus spectrum requires a break between the infrared and X-ray bands, and perhaps multiple breaks. This complex spectrum, which is an imprint of the particles injected into the nebula, has considerable consequences for the evolution of the broadband spectrum of 3C 58. We illustrate these effects and discuss the impact of these observations on the modeling of broadband spectra of pulsar wind nebulae.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2008ApJ...676L..33S&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2008/Slane/The%20Astrophysical%20Journal%202008%20Slane.pdf},
file = {{The Astrophysical Journal 2008 Slane.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2008/Slane/The Astrophysical Journal 2008 Slane.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/587031}}
}

@article{Slane:2002fh,
author = {Slane, Patrick O and Helfand, David J and Murray, Stephen S},
title = {{New Constraints on Neutron Star Cooling from Chandra Observations of 3C 58}},
journal = {The Astrophysical Journal},
year = {2002},
volume = {571},
pages = {L45},
month = may,
affiliation = {AA(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138), AB(Department of Astronomy, Columbia University, 1328 Pupin Hall, 550 West 120th Street, New York, NY 10027.), AC(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138)},
