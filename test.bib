@article{2002ApJ...571..866U,
author = {Uchiyama, Yasunobu and Takahashi, T and Aharonian, Felix A and Mattox, J R},
title = {{ASCA View of the Supernova Remnant $\gamma$ Cygni (G78.2+2.1): Bremsstrahlung X-Ray Spectrum from Loss-flattened Electron Distribution}},
