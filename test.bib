uri = {\url{papers2://publication/doi/10.1086/341179}}
}

@article{Slane:2004ip,
author = {Slane, Patrick and Helfand, David J and van der Swaluw, Eric and Murray, Stephen S},
title = {{New Constraints on the Structure and Evolution of the Pulsar Wind Nebula 3C 58}},
journal = {The Astrophysical Journal},
year = {2004},
volume = {616},
pages = {403},
month = nov,
affiliation = {AA(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138.), AB(Columbia Astrophysics Laboratory, Columbia University, 550 West 120th Street, New York, NY 10027.), AC(FOM-Institute for Plasma Physics Rijnhuizen, PO Box 1207, 3430 BE Nieuwegein, The Netherlands.), AD(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138.)},
doi = {10.1086/424814},
read = {Yes},
rating = {0},
date-added = {2010-09-23T12:36:17GMT},
date-modified = {2015-11-17T14:59:35GMT},
abstract = {We present an investigation of the spectral and spatial structure of the X-ray emission from 3C 58 based on a 350 ks observation with the Chandra X-ray Observatory. This deep image, obtained as part of the Chandra Large Project program, reveals new information on nearly all spatial scales in the pulsar wind nebula (PWN). On the smallest scales, we derive an improved limit of T<1.02×106 K for blackbody emission from the entire surface of the central neutron star (NS), confirming the need for rapid, nonstandard cooling in the stellar interior. Furthermore, we show that the data are consistent with emission from a light-element atmosphere with a similar temperature. Surrounding the NS, a toroidal structure with a jet is resolved, consistent with earlier measurements and indicative of an east-west orientation for the projected rotation axis of the pulsar. A complex of looplike X-ray filaments fills the nebula interior and corresponds well with structures seen in the radio band. Several of the structures coincide with optical filaments as well. The emission from the interior of the PWN, including the pulsar, jet, and filaments, is primarily nonthermal in nature. The power-law index steepens with radius, but appears to also show small azimuthal variations. The outermost regions of the nebula require a thermal emission component, confirming the presence of an ejecta-rich swept up shell.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2004ApJ...616..403S&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2004/Slane/The%20Astrophysical%20Journal%202004%20Slane.pdf},
file = {{The Astrophysical Journal 2004 Slane.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2004/Slane/The Astrophysical Journal 2004 Slane.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1086/424814}}
}

@article{Livingstone:2009br,
author = {Livingstone, Margaret A and Ransom, Scott M and Camilo, Fernando and Kaspi, Victoria M and Lyne, Andrew G and Kramer, Michael and Stairs, Ingrid H},
title = {{X-ray and Radio Timing of the Pulsar in 3C 58}},
journal = {The Astrophysical Journal},
year = {2009},
volume = {706},
pages = {1163},
month = dec,
affiliation = {AA(Department of Physics, McGill University, Montr{\'e}al, QC H3A 2T8, Canada maggie@physics.mcgill.ca), AB(National Radio Astronomy Observatory (NRAO), Charlottesville, VA 22903, USA ), AC(Columbia Astrophysics Laboratory, Columbia University, New York, NY 10027, USA ), AD(Department of Physics, McGill University, Montr{\'e}al, QC H3A 2T8, Canada ), AE(University of Manchester, Jodrell Bank Centre for Astrophysics, Manchester M13 9PL, UK ), AF(University of Manchester, Jodrell Bank Centre for Astrophysics, Manches...},
doi = {10.1088/0004-637X/706/2/1163},
read = {Yes},
rating = {0},
date-added = {2010-09-23T12:38:57GMT},
date-modified = {2015-11-17T14:59:35GMT},
abstract = {We present timing data spanning 6.4 yr for the young and energetic PSR J0205+6449, in the supernova remnant 3C 58. Data were obtained with the Rossi X-ray Timing Explorer, the Jodrell Bank Observatory, and the Robert C. Byrd Green Bank Telescope. We present phase-coherent timing analyses showing timing noise and two spin-up glitches with fractional frequency increases of ~3.4 × 10-7 near MJD 52555 and ~3.8 × 10-6 between MJDs 52777 and 53062. These glitches are unusually large if the pulsar was created in the historical supernova in 1181, as has been suggested. For the X-ray timing, we developed a new unbinned maximum likelihood method for determining pulse arrival times, which performs significantly better than the traditional binned techniques. In addition, we present an X-ray pulse profile analysis of four years of RXTE data showing that the pulsar is detected up to ~40 keV. We also present the first measurement of the phase offset between the radio and X-ray pulse for this source, showing that the radio pulse leads the X-ray pulse by phi = 0.10 $\pm$ 0.01 in phase. We compile all known measurements of the phase offsets between radio and X-ray and radio and $\gamma$-ray pulses for X-ray and $\gamma$-ray pulsars. We show that there is no relationship between pulse period and phase offset, supported by our measurement of the phase offset for PSR J0205+6449.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2009ApJ...706.1163L&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2009/Livingstone/The%20Astrophysical%20Journal%202009%20Livingstone.pdf},
file = {{The Astrophysical Journal 2009 Livingstone.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2009/Livingstone/The Astrophysical Journal 2009 Livingstone.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/706/2/1163}}
}

