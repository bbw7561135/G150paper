uri = {\url{papers2://publication/doi/10.1086/305011}}
}

@article{1995PhRvD..52.3265A,
author = {Atoyan, A M and Aharonian, Felix A and V{\"o}lk, H J},
title = {{Electrons and positrons in the galactic cosmic rays}},
journal = {Physical Review D (Particles},
year = {1995},
volume = {52},
number = {6},
pages = {3265--3275},
month = sep,
affiliation = {Yerevan Physics Institute, Yerevan, Armenia},
doi = {10.1103/PhysRevD.52.3265},
read = {Yes},
