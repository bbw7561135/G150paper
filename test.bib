journal = {Astronomy and Astrophysics},
year = {2003},
volume = {408},
pages = {237},
month = sep,
affiliation = {AA(University of Crete, Physics Department, PO Box 2208, 710 03 Heraklion, Crete, Greece)},
doi = {10.1051/0004-6361:20030955},
read = {Yes},
rating = {0},
date-added = {2012-11-29T18:20:09GMT},
date-modified = {2015-11-17T14:59:39GMT},
abstract = {The wide-field covered by the supernova remnant G 78.2+2.1 was observed in the optical emission lines of H$\alpha$+[N II] S II and O III. The flux calibrated images reveal several H II regions in the field which dominate the optical emission but we were able to identify possible areas of shock-heated emission through the H$\alpha$+[N II] and S II images. These are mainly found to the north-east of gamma Cygni as well as in the south and the morphology of the detected emission is patchy and diffuse. A few patchy structures are also detected in the medium ionization line image of [O III]. Long-slit spectra taken at one of the candidate positions verify that we have detected radiation from shock-heated gas (S II/H$\alpha$ ˜= 0.6). The estimated shock velocity lies below 100 km s-1, while the measured electron density of ˜700 cm-3 implies preshock cloud densities of ˜ 20 cm-3. High resolution maps in the infrared show that the optical emission, which may be associated with G 78.2+2.1, lies in areas relatively free of infrared emission. The interstellar extinction measured through the optical spectra is compatible with current estimates of the distance to the remnant. The optical data are in agreement with the explosion energy and interstellar medium density estimated from the X-ray data suggesting that the remnant is still in the adiabatic phase of its evolution. A second set of spectra taken in the north-west suggests that we are probably dealing with a foreground H II region.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2003A%26A...408..237M&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2003/Mavromatakis/Astronomy%20and%20Astrophysics%202003%20Mavromatakis.pdf},
file = {{Astronomy and Astrophysics 2003 Mavromatakis.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2003/Mavromatakis/Astronomy and Astrophysics 2003 Mavromatakis.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1051/0004-6361:20030955}}
}

