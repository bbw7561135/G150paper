file = {{arXiv 2009 Pineda.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2009/Pineda/arXiv 2009 Pineda.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/699/2/L134}}
}

