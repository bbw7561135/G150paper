@article{2002ApJ...571..545P,
author = {Protassov, Rostislav and van Dyk, David A and Connors, Alanna and Kashyap, Vinay L and Siemiginowska, Aneta},
title = {{Statistics: Handle with Care, Detecting Multiple Model Components with the Likelihood Ratio Test}},
year = {2002},
eprint = {astro-ph/0201547v1},
