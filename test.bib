@article{2014arXiv1402.5485T,
author = {Torres, Diego F and Cillis, Anal{\'\i}a and Mart{\'\i}n, Jonatan and de O{\~n}a Wilhelmi, Emma},
title = {{Time-dependent modeling of TeV-detected, young pulsar wind nebulae}},
year = {2014},
eprint = {1402.5485},
