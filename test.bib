pages = {189},
affiliation = {AA(Department of Physics, North Carolina State University, Box 8202, Raleigh NC 27695, USA), AB(Service d'Astrophysique, DSM/DAPNIA, CEA Saclay, 91191 Gif-sur-Yvette, France adecourchelle@cea.fr@cea.fr), AC(Service d'Astrophysique, DSM/DAPNIA, CEA Saclay, 91191 Gif-sur-Yvette, France jballet@cea.fr)},
doi = {10.1051/0004-6361:20034073},
read = {Yes},
rating = {0},
date-added = {2012-04-15T23:27:01GMT},
date-modified = {2015-11-17T14:59:39GMT},
abstract = {A number of supernova remnants (SNRs) show nonthermal X-rays assumed to be synchrotron emission from shock accelerated TeV electrons. The existence of these TeV electrons strongly suggests that the shocks in SNRs are sources of galactic cosmic rays (CRs). In addition, there is convincing evidence from broad-band studies of individual SNRs and elsewhere that the particle acceleration process in SNRs can be efficient and nonlinear. If SNR shocks are efficient particle accelerators, the production of CRs impacts the thermal properties of the shock heated, X-ray emitting gas and the SNR evolution. We report on a technique that couples nonlinear diffusive shock acceleration, including the backreaction of the accelerated particles on the structure of the forward and reverse shocks, with a hydrodynamic simulation of SNR evolution. Compared to models which ignore CRs, the most important hydrodynamical effects of placing a significant fraction of shock energy into CRs are larger shock compression ratios and lower temperatures in the shocked gas. We compare our results, which use an approximate description of the acceleration process, with a more complete model where the full CR transport equations are solved \citep[i.e.,][]{BKV2002}, and find excellent agreement for the CR spectrum summed over the SNR lifetime and the evolving shock compression ratio. The importance of the coupling between particle acceleration and SNR dynamics for the interpretation of broad-band continuum and thermal X-ray observations is discussed.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2004A%26A...413..189E&link_type=ABSTRACT},
local-url = {file://localhost/Users/elunejava/Dropbox/Papers2/Articles/2004/Ellison/Astronomy%20and%20Astrophysics%202004%20Ellison.pdf},
file = {{Astronomy and Astrophysics 2004 Ellison.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2004/Ellison/Astronomy and Astrophysics 2004 Ellison.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1051/0004-6361:20034073}}
}

@article{Ellison:2005kp,
