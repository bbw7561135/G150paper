@article{2005AdSpR..35.1041B,
author = {Baring, Matthew G and Ellison, Donald C and Slane, Patrick O},
title = {{Multiwavelength spectral models for SNR G347.3-0.5 from non-linear shock acceleration}},
journal = {Advances in Space Research},
year = {2005},
volume = {35},
