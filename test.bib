file = {{The Astrophysical Journal 2009 Abdo-3.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2009/Abdo/The Astrophysical Journal 2009 Abdo-3.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/706/2/1331}}
