}

@article{2004ASPC..317...66D,
author = {Dame, Thomas M and Thaddeus, P},
title = {{A Large Extension of the CfA Galactic CO Survey}},
journal = {Milky Way Surveys: The Structure and Evolution of our Galaxy},
year = {2004},
volume = {317},
pages = {66},
month = dec,
