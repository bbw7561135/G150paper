}

@article{Slane:2014wy,
author = {Slane, P and Bykov, A and Ellison, D C and Dubner, G and Castro, Daniel},
title = {{Supernova Remnants Interacting with Molecular Clouds: X-ray and Gamma-ray Signatures}},
