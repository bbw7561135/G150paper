file = {{arXiv 2011 Malyshev.pdf:/Users/elunejava/Dropbox/Papers2/Articles/2011/Malyshev/arXiv 2011 Malyshev.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/738/2/181}}
